#include <iostream>
#include <vector>
#include <string>
using namespace std;

class AttendanceSession {
private:
    string courseCode;
    string date;
    string startTime;
    int duration;

public:
    AttendanceSession(string code, string d, string time, int dur) {
        courseCode = code;
        date = d;
        startTime = time;
        duration = dur;
    }

    void displaySession() {
        cout << "\nCourse: " << courseCode;
        cout << "\nDate: " << date;
        cout << "\nStart Time: " << startTime;
        cout << "\nDuration: " << duration << " minutes\n";
    }
};